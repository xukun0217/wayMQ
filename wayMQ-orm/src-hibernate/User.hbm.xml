<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name='ananas.waymq.orm.model.User' table='T_USER' schema="waymq">

		<id name='id' type='string'>
			<column name='ID' length='50' />
			<generator class='ananas.waymq.orm.id_gen.UserIdGen' />

		</id>


		<property name='nickname' type='string'>
			<column name='C_NICKNAME' length='50' />
		</property>

		<property name='phoneId' type='string'>
			<column name='C_PHONE_ID' length='50' />
		</property>


	</class>
</hibernate-mapping>
